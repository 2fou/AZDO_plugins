{
    "manifestVersion": 1,
    "id": "feature-livrable-extension",
    "publisher": "AgileCoachingSolutionsFeatureAddon",

    "name": "Feature Livrable Control",
    "description": "Controle pour les features avec livrables",
    "public": false,
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "scopes": [
        "vso.work","vso.work_write"
    ],
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}