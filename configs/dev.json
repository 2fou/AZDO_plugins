{
  "baseUri": "https://localhost:3000",
  "id": "feature-livrable-extension",
  "public": false,
  "version": "1.0.192",
  "contributions": [
    {
      "id": "configuration",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-web.project-admin-hub-group"
      ],
      "properties": {
        "name": "Questionnaire Configuration",
        "order": 99,
        "uri": "dist/TestConfigurationPage/TestConfigurationPage.html"
      }
    },
    {
      "id": "progress-indicator-control",
      "type": "ms.vss-work-web.work-item-form-control",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "Progress Indicator",
        "uri": "dist/TestProgressIndicator/TestProgressIndicator.html",
        "height": 300
      }
    },
    {
      "id": "livrable-control",
      "type": "ms.vss-work-web.work-item-form-control",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "Livrable Control",
        "uri": "dist/TestExtensionComponent/TestExtensionComponent.html",
        "height": 500
      }
    },
    {
      "id": "setup-contribution",
      "type": "ms.vss-web.hub",
      "description": "Setup menu for configuring work items",
      "targets": [
        "ms.vss-web.project-admin-hub-group"
      ],
      "properties": {
        "order": 98,
        "name": "Work Item Setup",
        "uri": "dist/SetupComponent/SetupComponent.html"
      }
    },
    {
      "id": "create-workitems-contribution",
      "type": "ms.vss-web.action",
      "targets": [
        "ms.vss-work-web.work-item-toolbar-menu", 
        "ms.vss-work-web.backlog-item-menu",
        "ms.vss-work-web.work-item-context-menu"
      ],
      "properties": {
        "text": "Create Hierarchy",
        "registeredObjectId": "createHierarchy",
        "uri": "dist/CreateWorkItemsComponent/CreateWorkItemsComponent.html"
      }
    },
    {
      "id": "query-and-field-dashboard-widget",
      "type": "ms.vss-dashboards-web.widget",
      "description": "A widget for querying and interpreting JSON fields",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog"
      ],
      "properties": {
        "name": "Query and Field Dashboard Widget",
        "uri": "dist/QueryAndFieldDashboardWidget/QueryAndFieldDashboardWidget.html",
        "description": "Risk Profile Matrice",
        "catalogIconUrl": "dist/img/acceptable-risk.png",
        "previewImageUrl": "dist/img/acceptable-risk.png",
        "supportedSizes": [
          {
            "rowSpan": 5,
            "columnSpan": 3
          }
        ],
        "supportedScopes": [
          "project_team"
        ]
      }
    }
  ]
}